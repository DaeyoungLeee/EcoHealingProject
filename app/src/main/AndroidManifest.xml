        <activity
            android:name="kr.co.aiotlab.ecohealingproject.SwitchNaming.SwitchNameSetting_Power1"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name="kr.co.aiotlab.ecohealingproject.SwitchNaming.SwitchNameSetting_Power2"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name="kr.co.aiotlab.ecohealingproject.SwitchNaming.SwitchNameSetting_Power3"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name="kr.co.aiotlab.ecohealingproject.SwitchNaming.SwitchNameSetting_Power4"
